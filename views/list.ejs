<%- include('header'); -%>
<div class="box" id="heading">
    <h1><%= listTitle %> </h1>
</div>
<div class="box">
    <% for(i=0;i<listItem.length;i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%= listItem[i] %></p>
        </div>
        <%    } %>        
        <form action="/" method="post" class="">
            <input name="newItem" type="text" placeholder="New Item" autocomplete="off"/>
            <button type="submit" name="listtype" value="<%= listTitle %>">+</button>
        </form>
    </div>
        <%- include('footer'); -%>
    